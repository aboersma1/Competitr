@{
    ViewBag.Title = "programming";
}
@section koScripts {
<script type="text/javascript">
    ko.applyBindings(programmingViewmodel);
</script>
    }

<h2>@ViewBag.Title</h2>
<h3>@ViewBag.Message</h3>

<div class="btn-group">
    <button type="button" class="btn btn-default btn-lg dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
        Create a new workout <span class="glyphicon glyphicon-plus-sign"></span>
    </button>
    <ul class="dropdown-menu" role="menu" data-bind="foreach: workoutTypes">
        <li><a data-bind="text: WorkoutType"></a></li>
    </ul>
</div>

